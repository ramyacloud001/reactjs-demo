pipeline {
     agent any
     stages {
        stage("Build") {
            steps {
                sh "chmod +x -R ${env.WORKSPACE}"
                sh '/var/lib/jenkins/workspace/react-testv2/build.sh'
            }
        }
        stage("Deploy") {
            steps {
                sh '/var/lib/jenkins/workspace/react-testv2/deploy.sh'
            }
        }
    }
}
